{
  "name": "cardiac-support-group",
  "private": true,
  "description": "A Gatsby powered website for the Sunderland Cardiac Support Group - intended for both existing and prospective members.",
  "version": "0.3.0",
  "author": "Michael Dono",
  "dependencies": {
    "classnames": "^2.3.1",
    "gatsby": "^4.12.1",
    "gatsby-plugin-breadcrumb": "^12.3.0",
    "gatsby-plugin-react-helmet": "^5.12.1",
    "gatsby-plugin-sass": "^5.12.1",
    "gatsby-source-filesystem": "^4.12.1",
    "@michaeldono/gatsby-source-ghost": "^4.3.0",
    "prop-types": "^15.8.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0"
  },
  "devDependencies": {
    "eslint": "^7.32.0",
    "prettier": "^2.6.2"
  },
  "overrides": {
    "react-dev-utils": "^12.0.1",
    "cross-fetch": "^3.1.5",
    "cache-manager": "^3.6.1",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.5"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "clean": "gatsby clean",
    "build": "gatsby build",
    "restart": "npm run clean && gatsby develop",
    "format": "prettier --write src/**/*.{js,jsx}",
    "start": "gatsby develop",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\"",
    "schema": "set GATSBY_GRAPHQL_IDE=playground&& gatsby develop"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MichaelDono/cardiac-support-group"
  },
  "bugs": {
    "url": "https://github.com/MichaelDono/cardiac-support-group/issues"
  }
